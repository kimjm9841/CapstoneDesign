{
  "name": "HeYo - Exercise Manager",
  "description": "Start exercising that's perfect for you right now!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [ "alarms", "storage", "activeTab", "scripting",
     "notifications", "gcm", "history", "background" ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/HeYo16.png",
      "32": "/images/HeYo32.png",
      "48": "/images/HeYo48.png",
      "128": "/images/HeYo128.png"
    }
  },
  "icons": {
    "16": "/images/HeYo16.png",
    "32": "/images/HeYo32.png",
    "48": "/images/HeYo48.png",
    "128": "/images/HeYo128.png"
  },
  "options_page": "options.html"
}
